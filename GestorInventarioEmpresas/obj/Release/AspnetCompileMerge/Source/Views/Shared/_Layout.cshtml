@using Microsoft.AspNet.Identity
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Time Entry</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <meta content="A fully featured admin theme which can be used to build CRM, CMS, etc." name="description" />
    <meta content="Coderthemes" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta charset="ISO-8859-1">
    <link rel="stylesheet" href="~/Vendor/plugins/morris.js/morris.css">
    <link rel="shortcut icon" href="~/Content/Images/logo4.png">
    <link href="~/Vendor/plugins/notifications/notification.css" rel="stylesheet">
    <link href="~/Vendor/plugins/sweetalert/dist/sweetalert.css" rel="stylesheet" type="text/css">
    @*plugin forms*@
    <link href="~/Vendor/plugins/tagsinput/jquery.tagsinput.css" rel="stylesheet">
    <link href="~/Vendor/plugins/toggles/toggles.css" rel="stylesheet">
    <link href="~/Vendor/plugins/timepicker/bootstrap-timepicker.min.css" rel="stylesheet">
    <link href="~/Vendor/plugins/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css" rel="stylesheet">
    <link href="~/Vendor/plugins/colorpicker/colorpicker.css" rel="stylesheet" type="text/css">
    <link href="~/Vendor/plugins/jquery-multi-select/multi-select.css" rel="stylesheet" type="text/css">
    <link href="~/Vendor/plugins/select2/dist/css/select2.css" rel="stylesheet" type="text/css">
    <link href="~/Vendor/plugins/select2/dist/css/select2-bootstrap.css" rel="stylesheet" type="text/css">
    <link href="~/Vendor/plugins/bootstrap-touchspin/dist/jquery.bootstrap-touchspin.min.css" rel="stylesheet" />
    @*plugin  datatable ediable*@
    <link rel="stylesheet" href="~/vendor/plugins/magnific-popup/dist/magnific-popup.css">
    <link rel="stylesheet" href="~/vendor/plugins/jquery-datatables-editable/datatables.css">

    <link href="~/vendor/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="~/vendor/css/core.css" rel="stylesheet" type="text/css">
    <link href="~/vendor/css/icons.css" rel="stylesheet" type="text/css">
    <link href="~/vendor/css/components.css" rel="stylesheet" type="text/css">
    <link href="~/vendor/css/pages.css" rel="stylesheet" type="text/css">
    <link href="~/vendor/css/menu.css" rel="stylesheet" type="text/css">
    <link href="~/vendor/css/responsive.css" rel="stylesheet" type="text/css">

    <script src="~/vendor/js/modernizr.min.js"></script>

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->


</head>


<body class="fixed-left">

    <!-- Begin page -->
    <div id="wrapper">
        @if (Request.IsAuthenticated)
        {
            <!-- Top Bar Start -->
            <div class="topbar">
                <!-- LOGO -->
                <div class="topbar-left">
                    <div class="text-center">
                        <a href="/" class="logo"><i class="icon icon-accessibility"></i><span>Time Entry </span></a>
                    </div>
                </div>
                <!-- Button mobile view to collapse sidebar menu -->
                <div class="navbar navbar-default" role="navigation">
                    <div class="container">
                        <div class="">
                            <div class="pull-left">
                                <button type="button" class="button-menu-mobile open-left">
                                    <i class="fa fa-bars"></i>
                                </button>
                                <span class="clearfix"></span>
                            </div>


                            <ul class="nav navbar-nav navbar-right pull-right">

                                <li class="hidden-xs">
                                    <a href="#" id="btn-fullscreen" class="waves-effect"><i class="md md-crop-free"></i></a>
                                </li>

                                <li class="dropdown">

                                    <a href="" class="dropdown-toggle profile" data-toggle="dropdown" aria-expanded="true"><i class="md md-settings"></i></a>
                                    <ul class="dropdown-menu">
                                        <li><a href="javascript:void(0)"><i class="md md-face-unlock"></i> Profile</a></li>
                                        <li><a href="javascript:void(0)"><i class="md md-settings"></i> Settings</a></li>
                                        <li><a href="/Manage/ChangePassword"><i class="md md-lock"></i> Cambiar Constraseña</a></li>
                                        <li><a href="javascript:document.getElementById('logoutForm').submit()"><i class="md md-settings-power"></i> Salir</a></li>
                                    </ul>

                                </li>
                                @using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm" }))
                                {
                                    @Html.AntiForgeryToken()
                                }
                            </ul>
                        </div>
                        <!--/.nav-collapse -->
                    </div>
                </div>
            </div>
            <!-- Top Bar End -->
            <!-- ========== Left Sidebar Start ========== -->

            <div class="left side-menu">
                <div class="sidebar-inner slimscrollleft">
                    <div class="user-details">
                        <div class="pull-left">
                            <img src="~/vendor/images/users/avatar-1.jpg" alt="" class="thumb-md img-circle">
                        </div>
                        <div class="user-info">
                            <div class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false">@User.Identity.GetUserName() <span class="caret"></span></a>
                                <ul class="dropdown-menu">
                                    <li><a href="javascript:void(0)"><i class="md md-face-unlock"></i> Profile</a></li>
                                    <li><a href="javascript:void(0)"><i class="md md-settings"></i> Settings</a></li>
                                    <li><a href="/Manage/ChangePassword"><i class="md md-lock"></i> Cambiar Constraseña</a></li>
                                    <li><a href="javascript:document.getElementById('logoutForm').submit()"><i class="md md-settings-power"></i>Salir</a></li>
                                </ul>
                            </div>
                            @if (User.IsInRole("Administrador"))
                            {<p class="text-muted m-0">Administrador</p>}
                            @if (User.IsInRole("Usuario"))
                            {<p class="text-muted m-0">Usuario</p>}

                        </div>
                    </div>
                    <!--- Divider -->
                    <div id="sidebar-menu">
                        <ul>
                            <li>
                                <a href="/" class="waves-effect"><i class="md md-home"></i><span> Dashboard </span></a>
                            </li>
                            @if (User.IsInRole("Administrador"))
                            {
                                <li class="has_sub">
                                    <a href="#" class="waves-effect"><i class="md  md-account-child"></i><span>Usuarios </span><span class="pull-right"><i class="md md-add"></i></span></a>
                                    <ul class="list-unstyled">
                                        <li>@Html.ActionLink("Administrar", "Users", "Manage", routeValues: null, htmlAttributes: new { id = "registerLink" })</li>

                                    </ul>
                                </li>

                            }

                            <li class="has_sub">
                                @*  <a href="calendar.html" class="waves-effect"><i class="md md-event"></i><span> Horas </span></a>*@
                                <a href="#" class="waves-effect"><i class="md md-insert-invitation"></i> <span> Horas </span> <span class="pull-right"><i class="md md-add"></i></span></a>
                                <ul class="list-unstyled">
                                    <li><a href="@Url.Action("LoadHours", "Hours")"><i class="md md-file-upload"></i>Cargar Horas</a></li>
                                </ul>

                            </li>
                            @if (User.IsInRole("Administrador"))
                            {
                                <li class="has_sub">
                                    @*  <a href="calendar.html" class="waves-effect"><i class="md md-event"></i><span> Horas </span></a>*@
                                    <a href="#" class="waves-effect"><i class="md md-settings"></i> <span> Configuración </span> <span class="pull-right"><i class="md md-add"></i></span></a>
                                    <ul class="list-unstyled">
                                        <li><a href="@Url.Action("Index", "Companies")"><i class="ion-briefcase"></i>Empresas</a></li>
                                        <li><a href="@Url.Action("Index", "Projects")"><i class="md md-book"></i>Proyectos</a></li>
                                        <li><a href="@Url.Action("Index", "TaskTypes")"><i class="md md-layers"></i>Work Items</a></li>
                                    </ul>

                                </li>
                                }
                            
                            </ul>
                        <div class="clearfix"></div>
                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>
        }
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        <div class="content-page">
            <!-- Start content -->
            <div class="content">
                <div class="container">

                    <!-- Page-Title -->
                    <div class="row">
                        <div class="col-sm-12">
                            <h4 class="pull-left page-title">@ViewBag.Title</h4>
                            <ol class="breadcrumb pull-right">
                                <li><a href="#">Time Entry</a></li>
                                <li><a href="#"> @this.ViewContext.RouteData.Values["controller"].ToString()</a></li>
                                <li class="active">@ViewContext.RouteData.GetRequiredString("action")</li>
                            </ol>
                        </div>
                    </div>

                    <!-- Pls Remove -->
                    <div style="min-height:600px">
                        @RenderBody()
                    </div>


                </div> <!-- container -->

            </div> <!-- content -->

            <footer class="footer text-right">
                <p>&copy; @DateTime.Now.Year - Time Entry</p>
            </footer>

        </div>
        <!-- ============================================================== -->
        <!-- End Right content here -->
        <!-- ============================================================== -->
        <!-- Right Sidebar -->
        @if (Request.IsAuthenticated)
        {
            <div class="side-bar right-bar nicescroll">
                <h4 class="text-center">Chat</h4>
                <div class="contact-list nicescroll">
                    <ul class="list-group contacts-list">
                        <li class="list-group-item">
                            <a href="#">
                                <div class="avatar">
                                    <img src="~/vendor/images/users/avatar-1.jpg" alt="">
                                </div>
                                <span class="name">Chadengle</span>
                                <i class="fa fa-circle online"></i>
                            </a>
                            <span class="clearfix"></span>
                        </li>
                        <li class="list-group-item">
                            <a href="#">
                                <div class="avatar">
                                    <img src="~/vendor/images/users/avatar-2.jpg" alt="">
                                </div>
                                <span class="name">Tomaslau</span>
                                <i class="fa fa-circle online"></i>
                            </a>
                            <span class="clearfix"></span>
                        </li>
                        <li class="list-group-item">
                            <a href="#">
                                <div class="avatar">
                                    <img src="~/vendor/images/users/avatar-3.jpg" alt="">
                                </div>
                                <span class="name">Stillnotdavid</span>
                                <i class="fa fa-circle online"></i>
                            </a>
                            <span class="clearfix"></span>
                        </li>
                        <li class="list-group-item">
                            <a href="#">
                                <div class="avatar">
                                    <img src="~/vendor/images/users/avatar-4.jpg" alt="">
                                </div>
                                <span class="name">Kurafire</span>
                                <i class="fa fa-circle online"></i>
                            </a>
                            <span class="clearfix"></span>
                        </li>
                        <li class="list-group-item">
                            <a href="#">
                                <div class="avatar">
                                    <img src="~/vendor/images/users/avatar-5.jpg" alt="">
                                </div>
                                <span class="name">Shahedk</span>
                                <i class="fa fa-circle away"></i>
                            </a>
                            <span class="clearfix"></span>
                        </li>
                        <li class="list-group-item">
                            <a href="#">
                                <div class="avatar">
                                    <img src="~/vendor/images/users/avatar-6.jpg" alt="">
                                </div>
                                <span class="name">Adhamdannaway</span>
                                <i class="fa fa-circle away"></i>
                            </a>
                            <span class="clearfix"></span>
                        </li>
                        <li class="list-group-item">
                            <a href="#">
                                <div class="avatar">
                                    <img src="~/vendor/images/users/avatar-7.jpg" alt="">
                                </div>
                                <span class="name">Ok</span>
                                <i class="fa fa-circle away"></i>
                            </a>
                            <span class="clearfix"></span>
                        </li>
                        <li class="list-group-item">
                            <a href="#">
                                <div class="avatar">
                                    <img src="~/vendor/images/users/avatar-8.jpg" alt="">
                                </div>
                                <span class="name">Arashasghari</span>
                                <i class="fa fa-circle offline"></i>
                            </a>
                            <span class="clearfix"></span>
                        </li>
                        <li class="list-group-item">
                            <a href="#">
                                <div class="avatar">
                                    <img src="~/vendor/images/users/avatar-9.jpg" alt="">
                                </div>
                                <span class="name">Joshaustin</span>
                                <i class="fa fa-circle offline"></i>
                            </a>
                            <span class="clearfix"></span>
                        </li>
                        <li class="list-group-item">
                            <a href="#">
                                <div class="avatar">
                                    <img src="~/vendor/images/users/avatar-10.jpg" alt="">
                                </div>
                                <span class="name">Sortino</span>
                                <i class="fa fa-circle offline"></i>
                            </a>
                            <span class="clearfix"></span>
                        </li>
                    </ul>
                </div>
            </div>
        }
        <!-- /Right-bar -->


    </div>
    <!-- END wrapper -->

    <script>
        var resizefunc = [];
    </script>

    <!-- Main  -->
    <script src="~/vendor/js/jquery.min.js"></script>
    <script src="~/vendor/js/bootstrap.min.js"></script>
    <script src="~/vendor/js/detect.js"></script>
    <script src="~/vendor/js/fastclick.js"></script>
    <script src="~/vendor/js/jquery.slimscroll.js"></script>
    <script src="~/vendor/js/jquery.blockUI.js"></script>
    <script src="~/vendor/js/waves.js"></script>
    <script src="~/vendor/js/wow.min.js"></script>
    <script src="~/vendor/js/jquery.nicescroll.js"></script>
    <script src="~/vendor/js/jquery.scrollTo.min.js"></script>

    <script src="~/vendor/js/jquery.app.js"></script>

    <script src="~/Vendor/plugins/select2/dist/js/select2.min.js" type="text/javascript"></script>

    <script src="~/Vendor/plugins/tagsinput/jquery.tagsinput.min.js"></script>
    <script src="~/Vendor/plugins/toggles/toggles.min.js"></script>
    <script src="~/Vendor/plugins/timepicker/bootstrap-timepicker.min.js"></script>
    <script type="text/javascript" src="~/Vendor/plugins/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>
    <script type="text/javascript" src="~/Vendor/plugins/colorpicker/bootstrap-colorpicker.js"></script>
    <script type="text/javascript" src="~/Vendor/plugins/jquery-multi-select/jquery.multi-select.js"></script>
    <script type="text/javascript" src="~/Vendor/plugins/jquery-multi-select/jquery.quicksearch.js"></script>
    <script src="~/Vendor/plugins/bootstrap-inputmask/bootstrap-inputmask.min.js" type="text/javascript"></script>
    <script src="~/Vendor/plugins/bootstrap-touchspin/dist/jquery.bootstrap-touchspin.min.js" type="text/javascript"></script>
    <script src="https://momentjs.com/downloads/moment.js" type="text/javascript"></script>


    <!-- Plugin datatable editable -->
    <script src="~/Vendor/plugins/magnific-popup/dist/jquery.magnific-popup.min.js"></script>
    <script src="~/Vendor/plugins/jquery-datatables-editable/jquery.dataTables.js"></script>
    <script src="~/Vendor/plugins/datatables/dataTables.bootstrap.js"></script>
    <script src="~/Vendor/pages/datatables.editable.init.js"></script>
    <script src="~/Vendor/plugins/sweetalert/dist/sweetalert.min.js"></script>
    <script src="~/Vendor/pages/jquery.sweet-alert.init.js"></script>
    <script src="~/Vendor/plugins/notifyjs/dist/notify.min.js"></script>
    <script src="~/Vendor/plugins/notifications/notify-metro.js"></script>
    <script src="~/Vendor/plugins/notifications/notifications.js"></script>
    <!--form validation-->
    <script type="text/javascript" src="~/Vendor/plugins/jquery-validation/dist/jquery.validate.min.js"></script>

    <!--form validation init-->
    <script src="~/Vendor/pages/form-validation-init.js"></script>

    <script src="~/ModulosJs/UtilJs.js"></script>

    <script src="~/ModulosJs/CommonFunctions.js"></script>
    <script src="~/ModulosJs/AlertManager.js"></script>
    <script src="~/ModulosJs/BlockUIManager.js"></script>



    @Html.Partial("_AlertPartial")
    @RenderSection("scripts", required: false)
</body>
</html>